<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:muxc="using:Microsoft.UI.Xaml.Controls"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<!-- Implicit styles -->
	<Style BasedOn="{StaticResource MaterialAppBarButtonStyle}" TargetType="AppBarButton" />
	<Style BasedOn="{StaticResource MaterialBodyMedium}" TargetType="TextBlock" />
	<Style BasedOn="{StaticResource MaterialCalendarDatePickerStyle}" TargetType="CalendarDatePicker" />
	<Style BasedOn="{StaticResource MaterialCalendarViewStyle}" TargetType="CalendarView" />
	<Style BasedOn="{StaticResource MaterialCheckBoxStyle}" TargetType="CheckBox" />
	<Style BasedOn="{StaticResource MaterialContentDialogStyle}" TargetType="ContentDialog" />
	<Style BasedOn="{StaticResource MaterialComboBoxStyle}" TargetType="ComboBox" />
	<Style BasedOn="{StaticResource MaterialCommandBarStyle}" TargetType="CommandBar" />
	<Style BasedOn="{StaticResource MaterialDatePickerStyle}" TargetType="DatePicker" />
	<Style BasedOn="{StaticResource MaterialFilledButtonStyle}" TargetType="Button" />
	<Style BasedOn="{StaticResource MaterialFilledPasswordBoxStyle}" TargetType="PasswordBox" />
	<Style BasedOn="{StaticResource MaterialFilledTextBoxStyle}" TargetType="TextBox" />
	<Style BasedOn="{StaticResource MaterialFlyoutPresenterStyle}" TargetType="FlyoutPresenter" />
	<Style BasedOn="{StaticResource MaterialHyperlinkButtonStyle}" TargetType="HyperlinkButton" />
	<Style BasedOn="{StaticResource MaterialListViewItemStyle}" TargetType="ListViewItem" />
	<Style BasedOn="{StaticResource MaterialListViewStyle}" TargetType="ListView" />
	<Style BasedOn="{StaticResource MaterialMenuFlyoutItemStyle}" TargetType="MenuFlyoutItem" />
	<Style BasedOn="{StaticResource MaterialMenuFlyoutPresenterStyle}" TargetType="MenuFlyoutPresenter" />
	<Style BasedOn="{StaticResource MaterialNavigationViewStyle}" TargetType="muxc:NavigationView" />
	<Style BasedOn="{StaticResource MaterialNavigationViewItemStyle}" TargetType="muxc:NavigationViewItem" />
	<Style BasedOn="{StaticResource MaterialPipsPagerStyle}" TargetType="muxc:PipsPager" />
	<Style BasedOn="{StaticResource MaterialProgressBarStyle}" TargetType="muxc:ProgressBar" />
	<Style BasedOn="{StaticResource MaterialProgressRingStyle}" TargetType="muxc:ProgressRing" />
	<Style BasedOn="{StaticResource MaterialRadioButtonStyle}" TargetType="RadioButton" />
	<Style BasedOn="{StaticResource MaterialSliderStyle}" TargetType="Slider" />
	<Style BasedOn="{StaticResource MaterialTextToggleButtonStyle}" TargetType="ToggleButton" />
	<Style BasedOn="{StaticResource MaterialToggleSwitchStyle}" TargetType="ToggleSwitch" />
</ResourceDictionary>
