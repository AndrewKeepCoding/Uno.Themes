<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:toolkit="using:Uno.UI.Toolkit"
                    xmlns:um="using:Uno.Material"
                    mc:Ignorable="d">

	<!--#region Default FAB Brushes-->
	<StaticResource x:Key="M3MaterialFabForeground"
	                ResourceKey="OnPrimaryContainerBrush" />
	<StaticResource x:Key="M3MaterialFabBackground"
	                ResourceKey="PrimaryContainerBrush" />
	<StaticResource x:Key="M3MaterialFabDisabledBackground"
	                ResourceKey="SystemControlTransparentBrush" />
	<StaticResource x:Key="M3MaterialFabDisabledForeground"
	                ResourceKey="OnSurfaceDisabledBrush" />
	<StaticResource x:Key="M3MaterialFabPointerOverStateOverlay"
	                ResourceKey="OnPrimaryContainerHoverBrush" />
	<StaticResource x:Key="M3MaterialFabFocusedStateOverlay"
	                ResourceKey="OnPrimaryContainerFocusedBrush" />
	<StaticResource x:Key="M3MaterialFabPressedStateOverlay"
	                ResourceKey="OnPrimaryContainerPressedBrush" />
	<StaticResource x:Key="M3MaterialFabDisabledStateOverlay"
	                ResourceKey="OnSurfaceDisabledLowBrush" />
	<!--#endregion-->

	<!--#region Surface FAB Brushes-->
	<StaticResource x:Key="M3MaterialSurfaceFabForeground"
	                ResourceKey="PrimaryBrush" />
	<StaticResource x:Key="M3MaterialSurfaceFabBackground"
