<themes:BaseTheme x:Class="Uno.Material.MaterialTheme"
				  xmlns:themes="using:Uno.Themes"
				  xmlns:um="using:Uno.Material"
				  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
				  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<themes:BaseTheme.MergedDictionaries>
		
		<ResourceDictionary Source="ms-appx:///Uno.Material/Styles/Application/v2/SharedColorPalette.xaml" />

		<themes:PaletteCollection>
			<themes:PaletteCollection.MergedDictionaries>
				<ResourceDictionary Source="ms-appx:///Uno.Material/Styles/Application/v2/SharedColorPalette.xaml" />
			</themes:PaletteCollection.MergedDictionaries>
		</themes:PaletteCollection>

		<ResourceDictionary Source="ms-appx:///Uno.Material/Styles/Application/v2/SharedColors.xaml" />

	</themes:BaseTheme.MergedDictionaries>

</themes:BaseTheme>
